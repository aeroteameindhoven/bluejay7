<launch>
<!--- Run AMCL -->
   <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find bluejay_slam)/resource/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find bluejay_slam)/resource/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find bluejay_slam)/resource/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find bluejay_slam)/resource/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find bluejay_slam)/resource/base_local_planner_params.yaml" command="load" />
    <rosparam file="$(find bluejay_slam)/resource/move_base_params.yaml" command="load" />
 </node>

</launch>
